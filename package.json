{
  "name": "vue-leaflet-icon-wrapper",
  "description": "wraps icons making them dynamic",
  "version": "0.1.1",
  "main": "./src/index.js",
  "sideEffects": false,
  "keywords": [
    "js",
    "javascript"
  ],
  "license": "MIT",
  "author": "Eric Xanderson, Lorefolk LLC",
  "repository": {
    "type": "git",
    "url": "https://lily.thenovel.org/gitlab/cy/playground"
  },
  "dependencies": {
    "arrive": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@rollup/plugin-json": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-istanbul": "^5.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-jest": "^23.2.0",
    "chokidar": "^2.0.4",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-serve": "^1.0.2",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-uglify": "^6.0.3",
    "rollup-plugin-vue": "^5.1.4",
    "vue-template-compiler": "^2.6.10"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "babel-plugin-transform-es2015-modules-commonjs",
          "babel-plugin-transform-flow-strip-types",
          "babel-plugin-transform-class-properties",
          [
            "babel-plugin-transform-object-rest-spread",
            {
              "useBuiltIns": true
            }
          ]
        ],
        "presets": [
          "jest"
        ]
      }
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./.jestSetup.js",
    "testRegex": "./__tests__/.*\\.(js|jsx)$",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "modulePathIgnorePatterns": [
      "\\/\\."
    ]
  },
  "scripts": {
    "start": "npm run build",
    "build": "rollup --config",
    "build:watch": "rollup --config --watch \"./src\"",
    "lint": "./node_modules/.bin/eslint \"source/**/*.js\"",
    "server": "rollup --config rollup.serve.js",
    "test": "jest --colors",
    "test:watch": "jest --colors --watch --bail",
    "lint-staged": "lint-staged",
    "precommit": "npm run build && npm run test && npm run lint-staged"
  }
}
